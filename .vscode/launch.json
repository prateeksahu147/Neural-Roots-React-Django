    "version": "0.2.0",
    "configurations": [

        {
            "name": "Django",
            "type": "python",
            "request": "launch",
            "pythonPath": "/Users/prateeksahu/Documents/Neural-Roots/NeuralRoots-venv/bin/python",
            "program": "${workspaceRoot}/NeuralRoots_Server/manage.py",
            "args": [
                "runserver"              
            ],
            "django": true
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "urlFilter": "http://localhost:3001/*",
            "webRoot": "${workspaceFolder}/neural-roots-web"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/neural-roots-web"
        }
    ]
}